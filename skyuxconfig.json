{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-core",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "speedy"
    }
  },
  "app": {
    "styles": [
      "@skyux/docs-tools/css/docs-tools.css"
    ]
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
